# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'swiftui_demo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  #网络数据请求库
  pod 'Moya'
  pod 'Alamofire'
  #网络图片加载库
  pod 'SDWebImageSwiftUI'
  # json解析库
  pod 'SwiftyJSON'
  pod 'HandyJSON'
  
end
